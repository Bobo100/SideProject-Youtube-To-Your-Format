{
  "name": "youtube-to-your-format",
  "version": "1.0.0",
  "main": "main/main.js",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "build": "next build && electron-builder",
    "start": "next start",
    "lint": "next lint"
  },
  "build": {
    "appId": "com.example.youtubetoyoutformat",
    "files": [
      "main.js",
      "index.html",
      "preload.js",
      "renderer.js",
      "resources/**/*"
    ],
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.13",
    "@types/lodash": "^4.17.7",
    "electron-serve": "^2.0.0",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.1"
  }
}