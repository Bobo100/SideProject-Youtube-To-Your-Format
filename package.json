{
  "name": "youtube-to-your-format",
  "version": "1.0.0",
  "main": "main/main.mjs",
  "description": "youtube-to-your-format",
  "author": "Bobo",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "dev_electron": "electron .",
    "build": "next build && electron-builder",
    "build_next": "next build",
    "build_electron": "electron-builder",
    "postinstall": "node scripts/download-ffmpeg.js",
    "start": "serve -s out",
    "lint": "next lint"
  },
  "build": {
    "appId": "com.example.youtubetoyoutformat",
    "files": [
      "out/**/*",
      "main/**/*",
      "public/**/*",
      "resources/**/*"
    ],
    "extraResources": [
      {
        "from": "resources/",
        "to": "resources/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": "nsis",
      "signAndEditExecutable": false
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/lodash": "^4.17.7",
    "electron-serve": "^2.0.0",
    "extract-zip": "^2.0.1",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.2",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "http-server": "^14.1.1",
    "postcss": "^8.4.21",
    "sass": "^1.77.8",
    "serve": "^14.2.3",
    "tailwindcss": "^3.4.1"
  }
}